<apex:page controller="CustomForm">
    <apex:pageBlock >
        <apex:form >
            <apex:pageBlock title="Contact Form">
                <apex:pageBlockButtons >
                    <apex:commandButton action="{!save}" value="Save" />
                </apex:pageBlockButtons>
                <apex:pageBlockSection >
                    <apex:inputText value="{!conlist.Name}" />
                    <apex:inputText value="{!conlist.Student_ID__c}" />
                    <apex:inputText value="{!conlist.Student_Name__c}" />
                    <apex:inputText value="{!conlist.Student_Marks__c}" />
                </apex:pageBlockSection>
            </apex:pageBlock>
        </apex:form>
    </apex:pageBlock>
</apex:page>